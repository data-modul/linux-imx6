/dts-v1/;

#include "imx6q.dtsi"
#include "imx6qdl-dmo-ppc.dtsi"

/ {
	model = "Data Modul AG PPC Board-Q/D";
	compatible = "dmo,imx6q-ppc", "fsl,imx6q";

	ipum2m1: ipum2m@ipu2 {
		compatible = "fsl,imx-video-mem2mem";
		ipu = <&ipu2>;
		status = "okay";
	};
};

&sata {
	status = "okay";
};

&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2>;
	status = "okay";
};

